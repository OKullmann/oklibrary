set -o errexit
set -o nounset

program="Call_QueensRUCPct"
version="0.1"

N="${1:-16}"

make -B SETN="-DNN=$N" Queens_RUCP_ct Queens_RUCP_ct_debug > /dev/null

/usr/bin/time -f" %e %M" -o"/dev/stdout" -a ./Queens_RUCP_ct | tr -d "\n"
echo
