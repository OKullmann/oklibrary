# Recursively call make in all given directories.

# Version 0.1.0

SUBDIRS := $(shell find ./ -type d -name "MRunSolver_*")

.PHONY : all $(SUBDIRS)
all : $(SUBDIRS)

$(SUBDIRS) :
	$(MAKE) -C $@
