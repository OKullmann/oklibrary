<algconf>

    <node type="and" name="__dummy__" domain="[0]" ignore="true" >
        <!-- Lookahead weights -->
        <node type="and" name="w1" domain="[-1.0, 1.0]" prefix=""/>
        <node type="and" name="w2" domain="[0.001, 300.0]" prefix=""/>
        <node type="and" name="w3" domain="[0.001, 300.0]" prefix=""/>
        <node type="and" name="w4" domain="[0.001, 300.0]" prefix=""/>
        <node type="and" name="w5" domain="[0.001, 300.0]" prefix=""/>
        <node type="and" name="w6" domain="[0.001, 300.0]" prefix=""/>
    </node>

    <!-- Default configuration -->
    <seedgenome>
        <param name="__dummy__" value="0" />

        <!-- Lookahead weights-->
        <param name="w1" value="0.0"/>
        <param name="w2" value="2.0"/>
        <param name="w3" value="3.0"/>
        <param name="w4" value="4.0"/>
        <param name="w5" value="5.0"/>
        <param name="w6" value="6.0"/>
    </seedgenome>

</algconf>
