# USAGE:

# EXAMPLE:


set -o errexit
set -o nounset

program="SATWrap_minisat"
version="0.0.1"

: ${inputfile:="/dev/stdin"}
filename="$(basename ${inputfile})"

: ${outputfile:="/dev/stdout"}

timestamp="$(date +%s%N)"

: ${outputdirectory:="${program}_${filename}_${timestamp}"}

echo ${outputdirectory}

exit 0

cat ${inputfile} > ${outputfile}
