kill(f)$

/* ****************************
   * Unlabelled rooted trees  *
   ****************************
*/

okltest_rt_p(f) := block([x],
  assert(f([]) = true),
  assert(f(0) = false),
  assert(f({}) = false),
  assert(f(x) = false),
  assert(f([[]]) = true),
  assert(f([1]) = false),
  assert(f([[[]]]) = true),
  assert(f([[],[[]],[[],[]],[]]) = true),
  assert(f([[],[[]],[[],[]],[1,[]]]) = false),
  true)$

okltest_rt2_p(f) := (
  assert(f([]) = true),
  assert(f([[]]) = false),
  assert(f([[],[]]) = true),
  assert(f([[],[],[]]) = false),
  assert(f([[[]],[]]) = false),
  assert(f([[[],[]],[]]) = true),
  assert(f([[[],[]],[[]]]) = false),
  assert(f([[[],[]],[[],[]]]) = true),
  assert(f([[[],[],[]],[[],[]]]) = false),
  assert(f([[[],[]],[[],[],[]]]) = false),
  true)$

okltest_lvs(f) := (
  assert(f([]) = {[]}),
  assert(f([[]]) = {[1]}),
  assert(f([[[]]]) = {[1,1]}),
  assert(f([[],[]]) = {[1],[2]}),
  assert(f([[],[],[[]]]) = {[1],[2],[3,1]}),
  true)$

okltest_nds(f) := (
  assert(f([]) = {[]}),
  assert(f([[]]) = {[],[1]}),
  assert(f([[[]]]) = {[],[1],[1,1]}),
  assert(f([[],[]]) = {[],[1],[2]}),
  assert(f([[],[],[[]]]) = {[],[1],[2],[3],[3,1]}),
  true)$

okltest_subtree(f) := block([T,S],
  assert(f([],[]) = []),
  assert(f([[]],[]) = [[]]),
  assert(f([[]],[1]) = []),
  T : [[[]]], S : f(T,[1]),
  assert(S = [[]]),
  S[1] : [[],[]],
  assert(T=[[[[],[]]]]),
  true)$

okltest_dst(f) := (
  assert(dst([],[]) = {}),
  assert(dst([[]],[]) = {[1]}),
  assert(dst([[]],[1]) = {}),
  assert(dst([[],[],[]],[]) = {[1],[2],[3]}),
  assert(dst([[[]],[[],[]],[]],[2]) = {[2,1],[2,2]}),
  true)$


/* ****************************
   * Labelled rooted trees    *
   ****************************
*/

okltest_lrt_p(f) := block([x],
  assert(f(0) = false),
  assert(f([]) = false),
  assert(f([[]]) = true),
  assert(f([0]) = true),
  assert(f([0,[]]) = false),
  assert(f([0,0]) = false),
  assert(f([0,[0]]) = true),
  /* XXX */
  true)$

okltest_ndrep2l_lrt(f) := block([x,y],
  assert(f([], [x]) = x),
  assert(f([1], [x, [y]]) = y),
  assert(f([2], [0, [x], [y]]) = y),
  assert(f([1,2], [0, [0, [x], [y]]]) = y),
  true)$

okltest_l2ult(f) := block([x],
  assert(f([0]) = []),
  assert(f([x]) = []),
  assert(f([x,[1],[2]]) = [[],[]]),
  assert(f([x,[x,[1],[2]],[x,[x],[x,[x]]]]) = [[[],[]], [[],[[]]]]),
  true)$

okltest_lmap_lrt(f) := (
  assert(f([1], identity) = [1]),
  assert(f([1], lambda([x],x+1)) = [2]),
  assert(f([1,[2],[3]], identity) = [1,[2],[3]]),
  assert(f([1,[2],[3]], lambda([x],x+1)) = [2,[3],[4]]),
  true)$

okltest_rt2lrt_il(f) := block([x,y],
  assert(f([],[],x) = [x]),
  assert(f([],[1],x) = [x]),
  assert(f([[],[]], [x], y) = [x, [y], [y]]),
  assert(f([[],[]], [x,1], y) = [x, [y], [y]]),
  assert(f([[[]]], [1,2], x) = [1, [2, [x]]]),
  assert(f([[[]]], [1,2,y], x) = [1, [2, [x]]]),
  assert(f([[[]],[[]]], [1,2,3], x) = [1, [2,[x]], [3,[x]]]),
  true)$

okltest_ll_lrt(f) := (
  assert(f([1]) = {1}),
  assert(f([1,[2]]) = {2}),
  assert(f([1,[2],[3]]) = {2,3}),
  assert(f([1,[2,[4],[5]],[3]]) = {4,5,3}),
  true)$

okltest_lll_lrt(f) := (
  assert(f([1]) = [1]),
  assert(f([1,[2]]) = [2]),
  assert(f([1,[2],[3]]) = [2,3]),
  assert(f([1,[2,[4],[5]],[3]]) = [4,5,3]),
  true)$

okltest_il_lrt(f) := (
  assert(f([1]) = {}),
  assert(f([1,[2]]) = {1}),
  assert(f([1,[2],[3]]) = {1}),
  assert(f([1,[2,[4],[5]],[3]]) = {1,2}),
  true)$

okltest_l_lrt(f) := (
  assert(f([1]) = {1}),
  assert(f([1,[2]]) = {1,2}),
  assert(f([1,[2],[3]]) = {1,2,3}),
  assert(f([1,[2,[4],[5]],[3]]) = {1,2,4,5,3}),
  true)$

okltest_preorderl_lrt(f) := (
  assert(f([0]) = [0]),
  assert(f([0,[1]]) = [0,1]),
  assert(f([0,[1],[1]]) = [0,1,1]),
  assert(f([0,[1,[3],[4],[5]],[2]]) = [0,1,3,4,5,2]),
  true)$

okltest_levelorderl_lrt(f) := (
  assert(f([0]) = [0]),
  assert(f([0,[1]]) = [0,1]),
  assert(f([0,[1],[1]]) = [0,1,1]),
  assert(f([0,[1,[3],[4],[5]],[2]]) = [0,1,2,3,4,5]),
  true)$

okltest_depth_annotation_g_lrt(f) := block([x],
  assert(f([0],x) = [[0,x]]),
  assert(f([0,[1],[2]],x) = [[0,x],[[1,x+1]],[[2,x+1]]]),
  true)$

okltest_depth_annotation_lrt(f) := (
  assert(f([0]) = [[0,0]]),
  assert(f([0,[1,[2,[3]]]]) = [[0,0],[[1,1],[[2,2],[[3,3]]]]]),
  true)$


