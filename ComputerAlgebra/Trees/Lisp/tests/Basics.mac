okltest_g2lrt(f) := (
  assert(f([{1},{}],1) = [1]),
  assert(f([{1,2},{{1,2}}],1) = [1,[2]]),
  assert(f([{1,2},{{1,2}}],2) = [2,[1]]),
  assert(f([{1,2,3},{{1,2},{1,3}}],1) = [1,[2],[3]]),
  assert(f([{1,2,3},{{1,2},{1,3}}],2) = [2,[1,[3]]]),
  assert(f([{1,2,3},{{1,2},{1,3}}],3) = [3,[1,[2]]]),
  true)$


