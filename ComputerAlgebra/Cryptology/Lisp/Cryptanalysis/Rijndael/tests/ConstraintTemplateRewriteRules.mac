  assert(last(cl) = {-4,5,7,11,13,14,15,16}),
  assert(length(cl) = 294),
  if oklib_test_level = 0 then return(true),
  assert(rijnsbox_cnfp(fcl2fcs(cl2fcl(cl)))),
