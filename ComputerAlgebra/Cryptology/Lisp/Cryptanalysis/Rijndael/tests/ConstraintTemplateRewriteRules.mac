okltest_aes_bi_mixcolumn_cstr_cstl(f) := block([cst],
  cst : ["aes_mixcolumn_cst",create_list(i,i,1,64),nounify(n)],
  cstt_new : cstt_namespace_replace(cst,
    cstt_namespace_new(aes_bi_mixcolumn_namespace,cst)),
