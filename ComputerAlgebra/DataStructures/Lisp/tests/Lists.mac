okltest_rotate(f) := (
  assert(f([1,2,3],1) = [3,1,2]),
  assert(f([1,2,3],2) = [2,3,1]),
  for n : -3 thru 3 do
    assert(f([],n) = []),
  for n : -3 thru 3 do
    assert(f([1,2,3],3*n) = [1,2,3]),
  for n : -3 thru 3 do
    assert(f([1,2,3],n) = f([1,2,3],-3+n)),
  block([L1,L2],
   L1 : [1,2,3],
   L2 : f(L1,2),
   assert(L2 = [2,3,1]),
   L2[1] : 77,
   assert(L1 = [1,2,3])),
  true)$

okltest_stable_unique(f) := (
  assert(f([]) = []),
  assert(f([1]) = [1]),
  assert(f([1,2]) = [1,2]),
  assert(f([2,1]) = [2,1]),
  assert(f([1,1]) = [1]),
  assert(f([1,2,3]) = [1,2,3]),
  assert(f([2,1,3]) = [2,1,3]),
  assert(f([1,3,2]) = [1,3,2]),
  assert(f([1,2,1]) = [1,2]),
  assert(f([2,2,1]) = [2,1]),
  assert(f([3,1,1]) = [3,1]),
  assert(f([1,1,3]) = [1,3]),
  assert(f([1,1,1]) = [1]),
  assert(f([1,2,3,4,1,5,2,6,3,7,4,8,5]) = [1,2,3,4,5,6,7,8]),
  true)$

okltest_interleave_l(f) := block(
  for i : 1 thru 20 do
    assert(apply(f, create_list([],j,0,i)) = []),
  assert(f([1],[2]) = [1,2]),
  assert(f([1,3],[2,4]) = [1,2,3,4]),
  assert(f([1,3],[2,4,5]) = [1,2,3,4]),
  assert(f([1,4],[2,5],[3,6]) = [1,2,3,4,5,6]),
  assert(f([1,4,7],[2,5],[3,6]) = [1,2,3,4,5,6]),
  true)$

